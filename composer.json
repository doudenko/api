{
    "name": "doudenko/api",
    "version": "0.1.1",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Doudenko\\Api\\": "src/Api"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doudenko\\Api\\Tests\\": "tests/unit/Api"
        }
    },
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.9",
        "phpdocumentor/reflection-docblock": "^5.6",
        "symfony/property-access": "^7.2",
        "symfony/serializer": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "friendsofphp/php-cs-fixer": "*",
        "mockery/mockery": "^1.6",
        "dg/bypass-finals": "^1.8"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
