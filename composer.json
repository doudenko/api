{
    "name": "doudenko/api",
    "description": "Generalized implementation of an API client (a service that interacts with an external API).",
    "version": "0.1.2",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Doudenko\\Api\\": "src/Api"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doudenko\\Api\\Tests\\": "tests/unit/Api"
        }
    },
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.9",
        "phpdocumentor/reflection-docblock": "^5.6",
        "symfony/property-access": "^7.2",
        "symfony/serializer": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "friendsofphp/php-cs-fixer": "^v3.65",
        "mockery/mockery": "^1.6",
        "dg/bypass-finals": "^v1.8"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
